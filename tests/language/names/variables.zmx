let a; // Default declaration.

let b = 20; // Declaration with initial value.

let [c, d, e, f] = [4, 3, 2, 1]; // Multi-variable declaration.

assert(a == null);
assert(b == 20);
assert(c == 4);
assert(d == 3);
assert(e == 2);
assert(f == 1);

// One assignment.
a = 33;
assert(a == 33);

// Nested assignments.
a = b = c = 44;
assert(a == 44);
assert(a == b);
assert(a == c);

// Multi-variable assignment.
[a, b, c] = [11, 22, 33];
assert(a == 11);
assert(b == 22);
assert(c == 33);

// Never changed.
assert(d == 3);

const finalValue = 11 + 22 + 33 + 3 + 2 + 1;
assert(a + b + c + d + e + f == finalValue);
